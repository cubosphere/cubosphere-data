    CamBasis=BASIS_New()
    BASIS_Set(CamBasis,VECTOR_New(0.333791, 2.02944e-08, -0.942569), VECTOR_New(-0.393582, 0.908552, -0.139377), VECTOR_New(0.85638, 0.41753, 0.303267), VECTOR_New(115.122, 72.4367, 31.6251))

