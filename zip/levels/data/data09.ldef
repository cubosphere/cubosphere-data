      SIDE_SetVar(LEVEL_LastBlock()*6+5, "ReverseDirection", 0);

  LEVEL_AddBlock(-2,1,-5,"normal1");
    LEVEL_ChangeSide(LEVEL_LastBlock(),"front","lightbarrier");
