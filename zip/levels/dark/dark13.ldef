    CamBasis=BASIS_New()
    BASIS_Set(CamBasis,VECTOR_New(-0.607445, -1.04503e-07, -0.794364), VECTOR_New(-0.255606, 0.94678, 0.195457), VECTOR_New(0.752102, 0.321776, -0.575116), VECTOR_New(85.4646, 33.847, -214.57))

