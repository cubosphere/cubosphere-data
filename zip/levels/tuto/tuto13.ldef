    BASIS_Set(CamBasis,VECTOR_New(-0.0773097, -5.62783e-08, 0.996997), VECTOR_New(0.547036, 0.83601, 0.0424204), VECTOR_New(-0.8335, 0.548678, -0.0646319), VECTOR_New(-379.459, 190.677, -372.701))

