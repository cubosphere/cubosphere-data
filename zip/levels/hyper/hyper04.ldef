    BASIS_Set(CamBasis,VECTOR_New(0.565288, 7.83722e-10, 0.824874), VECTOR_New(0.456026, 0.833245, -0.312519), VECTOR_New(-0.687324, 0.552849, 0.471029), VECTOR_New(103.348, 126.662, -179.122))

